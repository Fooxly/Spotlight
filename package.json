{
    "name": "@fooxly/spotlight",
    "version": "2.0.1-beta.2",
    "description": "Helping your web development expierence with spotlight",
    "private": false,
    "main": "./lib/cjs/index.js",
    "module": "./lib/esm/index.js",
    "exports": {
        ".": [
            {
                "import": "./lib/esm/index.js",
                "require": "./lib/cjs/index.js",
                "default": "./lib/cjs/index.js"
            },
            "./lib/cjs/index.js"
        ],
        "./pre-18": [
            {
                "import": "./lib/esm/pre-18.js",
                "require": "./lib/cjs/pre-18.js",
                "default": "./lib/cjs/pre-18.js"
            },
            "./lib/cjs/pre-18.js"
        ],
        "./main.css": [
            {
                "import": "./lib/cjs/styles/main.css",
                "require": "./lib/cjs/styles/main.css",
                "default": "./lib/cjs/styles/main.css"
            },
            "./lib/cjs/styles/main.css"
        ]
    },
    "files": [
        "/lib",
        "/server/dist"
    ],
    "keywords": [
        "spotlight",
        "fooxly",
        "web",
        "development",
        "helper",
        "ease-of-use",
        "extension"
    ],
    "author": "Fooxly <hello@fooxly.com> (https://developers.fooxly.com)",
    "homepage": "https://github.com/Fooxly/Spotlight",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Fooxly/Spotlight.git"
    },
    "bugs": {
        "url": "https://github.com/Fooxly/Spotlight/issues",
        "email": "bugs@fooxly.com"
    },
    "bin": {
        "spotlight": "./lib/cjs/server/index.js"
    },
    "browserslist": {
        "production": [
            "last 1 version",
            "> 0.4%",
            "not dead",
            "not ie <= 10",
            "not baidu > 1",
            "not Android > 1",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 safari version"
        ]
    },
    "scripts": {
        "debug": "concurrently --kill-others \"yarn debug:cjs\" \"yarn debug:esm\" \"yarn debug:cpy\"",
        "debug:cjs": "concurrently --kill-others \"tsc -w --project tsconfig.cjs.json\" \"sass --watch src:lib/cjs\" \"tsc-alias -w -p tsconfig.cjs.json\"",
        "debug:esm": "concurrently --kill-others \"tsc -w --project tsconfig.json\" \"sass --watch src:lib/esm\" \"tsc-alias -w -p tsconfig.json\"",
        "debug:cpy": "nodemon --watch src --ext ts,tsx,json,scss --exec 'sleep 1 && yarn build:cpy'",
        "clean": "rm -rf lib",
        "build": "yarn clean && yarn build:esm && yarn build:cjs && yarn build:cpy",
        "build:esm": "tsc --project tsconfig.json && sass src:lib/esm && tsc-alias -p tsconfig.json",
        "build:cjs": "tsc --project tsconfig.cjs.json && sass src:lib/cjs && tsc-alias -p tsconfig.cjs.json",
        "build:cpy": "cp lib/cjs/pre-18.d.ts pre-18.d.ts",
        "typecheck": "tsc --noEmit",
        "lint": "eslint --ext ts,tsx src",
        "lint-fix": "yarn lint --fix"
    },
    "dependencies": {
        "body-parser": "^1.20.0",
        "command-line-args": "^5.2.1",
        "cors": "^2.8.5",
        "express": "^4.17.3",
        "fuse.js": "^6.5.3",
        "react-hotkeys-hook": "^3.4.4",
        "ws": "^8.8.0"
    },
    "devDependencies": {
        "@fooxly/eslint-config": "https://github.com/Fooxly/eslint-config",
        "@fooxly/eslint-config-react": "https://github.com/Fooxly/eslint-config-react",
        "@types/command-line-args": "^5.2.0",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/react": "^18.0.2",
        "@types/react-dom": "^18.0.0",
        "@types/ws": "^8.5.3",
        "concurrently": "^7.1.0",
        "jest": "^27.5.1",
        "nodemon": "^2.0.19",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "sass": "^1.53.0",
        "tsc-alias": "^1.6.11",
        "typescript": "^4.6.3"
    },
    "peerDependencies": {
        "react": "15 - 18",
        "react-dom": "15 - 18"
    }
}
